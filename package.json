{
  "scripts": {
    "dev": "pnpm build && pnpm start",
    "register": "pnpm build -r && pnpx tsx ./bot.gen.ts",
    "build": "pnpx tsx ./node_modules/@dressed/cmd/mod.ts build",
    "start": "pnpx tsx ./server.ts",
    "bundle": "pnpx esbuild server.ts > bundle.js --bundle --platform=node"
  },
  "dependencies": {
    "@dressed/cmd": "npm:@jsr/dressed__cmd@^0.1.9",
    "@dressed/dressed": "npm:@jsr/dressed__dressed@^1.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2"
  }
}
